# Copy to .env and fill in
# Server port (in dev, uses PORT exactly, no fallback)
PORT=3002
NODE_ENV=development
# Frontend: set VITE_MYSQL_API_URL in project root .env to match (e.g. http://localhost:3002)
MYSQL_HOST=localhost
MYSQL_PORT=3306
MYSQL_USER=root
MYSQL_PASSWORD=
MYSQL_DATABASE=anef_field_connect
JWT_SECRET=your-secret-key-change-in-production
# CORS: frontend origin (Vite default port 8084)
CORS_ORIGIN=http://localhost:8084
# OpenAI API Key for intelligent transcription (optional)
OPENAI_API_KEY=sk-your-openai-api-key-here
